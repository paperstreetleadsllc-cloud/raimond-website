{
  "redirects": [
    { "source": "/(.*)", "destination": "https://raimondai.com/$1", "has": [{ "type": "host", "value": "www.raimondai.com" }], "permanent": true }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}